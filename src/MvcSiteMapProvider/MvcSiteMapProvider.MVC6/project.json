{
  "version": "1.0.0-*",
  "description": "MvcSiteMapProvider is a tool that provides flexible menus, breadcrumb trails, and SEO features for the ASP.NET MVC framework, similar to the ASP.NET 2.0 SiteMapProvider model. The project is hosted on http://github.com/maartenba/MvcSiteMapProvider.",
  "authors": [ "Maarten Balliauw", "Shad Storhaug" ],
  "tags": [ "mvc", "mvc6", "aspnet5", "sitemap", "mvcsitemap", "menu", "breadcrumb", "navigation", "mvcsitemapprovider" ],
  "projectUrl": "http://github.com/maartenba/MvcSiteMapProvider",
  "licenseUrl": "https://github.com/maartenba/MvcSiteMapProvider/blob/master/LICENSE.md",
  "iconUrl": "https://raw.githubusercontent.com/maartenba/MvcSiteMapProvider/master/map-icon.gif",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta7",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-beta7",
    "Microsoft.AspNet.Routing": "1.0.0-beta7"
  },

  "compile": "../MvcSiteMapProvider/**/*.cs",

  "compilationOptions": {
    "define": ["MVC6", "NET46"]
  },

  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-beta-23225",
        "System.Collections": "4.0.11-beta-23225",
        "System.Linq": "4.0.1-beta-23225",
        "System.Runtime": "4.0.21-beta-23225",
        "System.Threading": "4.0.11-beta-23225"
      }
    }
  }
}
